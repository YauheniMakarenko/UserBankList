<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>DataofAllUsers</servlet-name>
        <servlet-class>com.userbanklist.servlets.DataofAllUsers</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DataofAllUsers</servlet-name>
        <url-pattern>/dataAllUsers</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletAndJSP</servlet-name>
        <servlet-class>com.userbanklist.servlets.RichestUserAndBankAccountAmount</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletAndJSP</servlet-name>
        <url-pattern>/ServletAndJSP</url-pattern>
    </servlet-mapping>
</web-app>
